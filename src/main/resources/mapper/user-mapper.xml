<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

    <mapper namespace="com.nineone.nocm.mapper.user">

    <insert id="insertUser">
    	insert member (id,name,phone,email,icon)
    		values(#{id},#{name},#{phone},#{email},#{icon})
    </insert>
    
    <select id="getUserid" resultType="java.lang.String">
    	select userid from member where userid = #{id}
    </select>
    
    <select id="getUserfindByEmail" resultType="java.util.Map">
        select * from member where email = #{email}
    </select>
    <select id="getUserById" resultType="com.nineone.nocm.domain.User">
        select * from member where id = #{id}
    </select>
    </mapper>