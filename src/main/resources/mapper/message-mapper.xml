<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.nineone.nocm.mapper.message">
    
    <insert id="insertMessage" useGeneratedKeys="true" keyProperty="id">
    	insert message (channel_id,content,sender)
			values (#{channel_id},#{content},#{sender}) 
    </insert>
    
    <select id="getMessageList">
   
    </select>
    
    </mapper>
    